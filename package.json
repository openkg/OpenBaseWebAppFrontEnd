{
  "name": "openbase",
  "version": "0.1.0",
  "private": true,
  "engineStrict": true,
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "goku": "goku xbox --archive-git-url git@v9.git.n.xiaomi.com:mcfe/openbase-archive.git",
    "start": "node scripts/start.js",
    "server": "node src/server/server.js",
    "build": "cross-env PUBLISH_ENV=dev node scripts/build.js",
    "buildFormal": "cross-env PUBLISH_ENV=formal node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "flow": "flow",
    "lint": "eslint --cache --cache-location=.eslintcache --ext .js src/",
    "lintFix": "eslint --fix --ext .js src/",
    "prepush": "npm run lint"
  },
  "dependencies": {
    "cropperjs": "^1.4.2",
    "echarts": "^4.2.0-rc.1",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.15",
    "lodash.isplainobject": "^4.0.6",
    "react": "^16.5.1",
    "react-cropper": "^1.0.1",
    "react-css-modules": "^4.7.7",
    "react-dom": "^16.5.1",
    "react-html5-form": "^1.0.9",
    "react-redux": "^5.0.7",
    "react-router": "^3.2.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "swiper": "^4.4.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.1.2",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-import": "^1.9.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-react-app": "^3.0.0",
    "babel-runtime": "^6.23.0",
    "case-sensitive-paths-webpack-plugin": "^1.1.4",
    "chalk": "^1.1.3",
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^6.0.3",
    "css-loader": "^0.28.1",
    "dotenv": "^4.0.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.81.0",
    "fs-extra": "^3.0.1",
    "get-port": "^3.1.0",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.14.3",
    "jest": "^20.0.3",
    "json-server": "^0.14.0",
    "node-sass": "^4.9.3",
    "object-assign": "^4.1.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-flexbugs-fixes": "^3.0.0",
    "postcss-loader": "^2.0.5",
    "prettier": "^1.19.1",
    "promise": "^7.1.1",
    "prop-types": "^15.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.17.0",
    "sw-precache-webpack-plugin": "^0.9.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-manifest-plugin": "^1.1.0",
    "whatwg-fetch": "^2.0.3",
    "write-file-webpack-plugin": "^4.2.0"
  },
  "proxy": "https://openbase.ai.xiaomi.com",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  }
}
